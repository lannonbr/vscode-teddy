{
  "<include …>": {
    "prefix": "include",
    "body": "<include src='$1'>$2</include>",
    "description": "<include> element. Import external templates."
  },
  "<arg>": {
    "prefix": "arg",
    "body": "<arg>$1</arg>",
    "description": "<include> <arg> argument."
  },
  "<if …>": {
    "prefix": "if",
    "body": "<if $1>\n\t$2\n</if>",
    "description": "<if> block."
  },
  "<else>": {
    "prefix": "else",
    "body": "<else>\n\t$1\n</else>",
    "description": "<else> block."
  },
  "<if …><else>": {
    "prefix": "ifelse",
    "body": ["<if $1>\n\t$2\n</if>", "<else>\n\t$3\n</else>"],
    "description": "<if> <else> block."
  },
  "<unless …>": {
    "prefix": "unless",
    "body": "<unless $1>\n\t$2\n</unless>",
    "description": "<unless> block. Check for the non-presence of something."
  },
  "<elseif …>": {
    "prefix": "elseif",
    "body": "<elseif $1>\n\t$2\n</elseif>",
    "description": "<elseif> block."
  },
  "<elseunless …>": {
    "prefix": "elseunless",
    "body": "<elseunless $1>\n\t$2\n</elseunless>",
    "description": "<elseunless> block. Check for the non-presence of something."
  },
  "if-…": {
    "prefix": "onelineif",
    "body": "if-$1 true=\"$2\" false=\"$3\"",
    "description": "One-line version of <if>."
  },
  "<loop …>": {
    "prefix": "loop",
    "body": "<loop through='$1' val='$2'>\n\t$3\n</loop>",
    "description": "<loop> block. Loop through an array or object of data."
  },
  "<loop …> with key": {
    "prefix": "loopkey",
    "body": "<loop through='$1' key='$2' val='$3'>\n\t$4\n</loop>",
    "description": "<loop> block with a key attribute. Loop through an array or object of data."
  },
  "<cache>": {
    "prefix": "cache",
    "body": "<cache name='$1'>\n\t$2\n</cache>",
    "description": "<cache> block. Cache template partial."
  },
  "<cache> with key": {
    "prefix": "cachekey",
    "body": "<cache name='$1' key='$2'>\n\t$3\n</cache>",
    "description": "<cache> block with a key attribute. Cache template partial."
  },
  "<cache> with maxAge": {
    "prefix": "cachemaxage",
    "body": "<cache name='$1' key='$2' maxAge='$3'>\n\t$4\n</cache>",
    "description": "<cache> block with key and maxAge attributes. Cache template partial."
  },
  "<cache> with maxCaches": {
    "prefix": "cachemaxcaches",
    "body": "<cache name='$1' key='$2' maxCaches='$3'>\n\t$4\n</cache>",
    "description": "<cache> block with key and maxCaches attributes. Cache template partial."
  },
  "<cache> with everything": {
    "prefix": "cacheall",
    "body": "<cache name='$1' key='$2' maxAge='$3' maxCaches='$4'>\n\t$5\n</cache>",
    "description": "<cache> block with key, maxAge, and maxCaches attributes. Cache template partial."
  }
}
